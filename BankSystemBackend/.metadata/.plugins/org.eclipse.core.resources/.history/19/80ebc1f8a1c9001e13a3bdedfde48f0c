package com.RSstudies.Services;

import java.util.List;

import com.RSstudies.models.Account;
import com.RSstudies.models.Loan;

public interface LoanService {
	String applyLoan(Account account, double amount);
	String approveLoan(Loan loan);
	String rejectLoan(Loan loan);
	List<Loan> getLoans(Account account);
}
